:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x6019d3a82220 .scope module, "pwm_tb" "pwm_tb" 2 2;
 .timescale -9 -12;
v0x6019d3a9cde0_0 .var "bus_addr", 31 0;
v0x6019d3a9cec0_0 .var "bus_wdata", 31 0;
v0x6019d3a9cf90_0 .var "bus_we", 0 0;
v0x6019d3a9d090_0 .var "clk", 0 0;
v0x6019d3a9d160_0 .net "pwm_out", 0 0, v0x6019d3a9c810_0;  1 drivers
v0x6019d3a9d250_0 .var "rst_n", 0 0;
S_0x6019d3a823b0 .scope module, "dut" "pwm_ip" 2 13, 3 1 0, S_0x6019d3a82220;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /INPUT 1 "bus_we";
    .port_info 3 /INPUT 32 "bus_addr";
    .port_info 4 /INPUT 32 "bus_wdata";
    .port_info 5 /OUTPUT 32 "bus_rdata";
    .port_info 6 /OUTPUT 1 "pwm_out";
P_0x6019d3a68dc0 .param/l "CTRL" 1 3 13, C4<00000000000000000000000000000000>;
P_0x6019d3a68e00 .param/l "DUTY" 1 3 15, C4<00000000000000000000000000001000>;
P_0x6019d3a68e40 .param/l "PERIOD" 1 3 14, C4<00000000000000000000000000000100>;
P_0x6019d3a68e80 .param/l "STATUS" 1 3 16, C4<00000000000000000000000000001100>;
v0x6019d3a6a710_0 .net "bus_addr", 31 0, v0x6019d3a9cde0_0;  1 drivers
v0x6019d3a6a7b0_0 .var "bus_rdata", 31 0;
v0x6019d3a9c130_0 .net "bus_wdata", 31 0, v0x6019d3a9cec0_0;  1 drivers
v0x6019d3a9c220_0 .net "bus_we", 0 0, v0x6019d3a9cf90_0;  1 drivers
v0x6019d3a9c2e0_0 .net "clk", 0 0, v0x6019d3a9d090_0;  1 drivers
v0x6019d3a9c3f0_0 .var "cnt", 31 0;
v0x6019d3a9c4d0_0 .var "duty_reg", 31 0;
v0x6019d3a9c5b0_0 .var "en", 0 0;
v0x6019d3a9c670_0 .var "period_reg", 31 0;
v0x6019d3a9c750_0 .var "pol", 0 0;
v0x6019d3a9c810_0 .var "pwm_out", 0 0;
v0x6019d3a9c8d0_0 .net "rst_n", 0 0, v0x6019d3a9d250_0;  1 drivers
E_0x6019d3a7aee0/0 .event anyedge, v0x6019d3a6a710_0, v0x6019d3a9c750_0, v0x6019d3a9c5b0_0, v0x6019d3a9c670_0;
E_0x6019d3a7aee0/1 .event anyedge, v0x6019d3a9c4d0_0, v0x6019d3a9c3f0_0;
E_0x6019d3a7aee0 .event/or E_0x6019d3a7aee0/0, E_0x6019d3a7aee0/1;
E_0x6019d3a7b8e0 .event anyedge, v0x6019d3a9c5b0_0, v0x6019d3a9c750_0, v0x6019d3a9c3f0_0, v0x6019d3a9c4d0_0;
E_0x6019d3a7bbf0/0 .event negedge, v0x6019d3a9c8d0_0;
E_0x6019d3a7bbf0/1 .event posedge, v0x6019d3a9c2e0_0;
E_0x6019d3a7bbf0 .event/or E_0x6019d3a7bbf0/0, E_0x6019d3a7bbf0/1;
S_0x6019d3a9ca70 .scope task, "write" "write" 2 23, 2 23 0, S_0x6019d3a82220;
 .timescale -9 -12;
v0x6019d3a9cc20_0 .var "addr", 31 0;
v0x6019d3a9cd00_0 .var "data", 31 0;
TD_pwm_tb.write ;
    %load/vec4 v0x6019d3a9cc20_0;
    %store/vec4 v0x6019d3a9cde0_0, 0, 32;
    %load/vec4 v0x6019d3a9cd00_0;
    %store/vec4 v0x6019d3a9cec0_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6019d3a9cf90_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6019d3a9cf90_0, 0, 1;
    %end;
    .scope S_0x6019d3a823b0;
T_1 ;
    %wait E_0x6019d3a7bbf0;
    %load/vec4 v0x6019d3a9c8d0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x6019d3a9c5b0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x6019d3a9c750_0, 0;
    %pushi/vec4 1, 0, 32;
    %assign/vec4 v0x6019d3a9c670_0, 0;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x6019d3a9c4d0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x6019d3a9c220_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x6019d3a6a710_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/u;
    %jmp/1 T_1.4, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 32;
    %cmp/u;
    %jmp/1 T_1.5, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 32;
    %cmp/u;
    %jmp/1 T_1.6, 6;
    %jmp T_1.7;
T_1.4 ;
    %load/vec4 v0x6019d3a9c130_0;
    %parti/s 1, 0, 2;
    %assign/vec4 v0x6019d3a9c5b0_0, 0;
    %load/vec4 v0x6019d3a9c130_0;
    %parti/s 1, 1, 2;
    %assign/vec4 v0x6019d3a9c750_0, 0;
    %jmp T_1.7;
T_1.5 ;
    %load/vec4 v0x6019d3a9c130_0;
    %cmpi/u 1, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_1.8, 5;
    %load/vec4 v0x6019d3a9c130_0;
    %assign/vec4 v0x6019d3a9c670_0, 0;
T_1.8 ;
    %jmp T_1.7;
T_1.6 ;
    %load/vec4 v0x6019d3a9c130_0;
    %assign/vec4 v0x6019d3a9c4d0_0, 0;
    %jmp T_1.7;
T_1.7 ;
    %pop/vec4 1;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x6019d3a823b0;
T_2 ;
    %wait E_0x6019d3a7bbf0;
    %load/vec4 v0x6019d3a9c8d0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x6019d3a9c3f0_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x6019d3a9c5b0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.2, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x6019d3a9c3f0_0, 0;
    %jmp T_2.3;
T_2.2 ;
    %load/vec4 v0x6019d3a9c3f0_0;
    %load/vec4 v0x6019d3a9c670_0;
    %subi 1, 0, 32;
    %cmp/e;
    %jmp/0xz  T_2.4, 4;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x6019d3a9c3f0_0, 0;
    %jmp T_2.5;
T_2.4 ;
    %load/vec4 v0x6019d3a9c3f0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x6019d3a9c3f0_0, 0;
T_2.5 ;
T_2.3 ;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x6019d3a823b0;
T_3 ;
    %wait E_0x6019d3a7b8e0;
    %load/vec4 v0x6019d3a9c5b0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x6019d3a9c750_0;
    %flag_set/vec4 8;
    %jmp/0 T_3.2, 8;
    %pushi/vec4 1, 0, 1;
    %jmp/1 T_3.3, 8;
T_3.2 ; End of true expr.
    %pushi/vec4 0, 0, 1;
    %jmp/0 T_3.3, 8;
 ; End of false expr.
    %blend;
T_3.3;
    %store/vec4 v0x6019d3a9c810_0, 0, 1;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x6019d3a9c3f0_0;
    %load/vec4 v0x6019d3a9c4d0_0;
    %cmp/u;
    %jmp/0xz  T_3.4, 5;
    %load/vec4 v0x6019d3a9c750_0;
    %flag_set/vec4 8;
    %jmp/0 T_3.6, 8;
    %pushi/vec4 0, 0, 1;
    %jmp/1 T_3.7, 8;
T_3.6 ; End of true expr.
    %pushi/vec4 1, 0, 1;
    %jmp/0 T_3.7, 8;
 ; End of false expr.
    %blend;
T_3.7;
    %store/vec4 v0x6019d3a9c810_0, 0, 1;
    %jmp T_3.5;
T_3.4 ;
    %load/vec4 v0x6019d3a9c750_0;
    %flag_set/vec4 8;
    %jmp/0 T_3.8, 8;
    %pushi/vec4 1, 0, 1;
    %jmp/1 T_3.9, 8;
T_3.8 ; End of true expr.
    %pushi/vec4 0, 0, 1;
    %jmp/0 T_3.9, 8;
 ; End of false expr.
    %blend;
T_3.9;
    %store/vec4 v0x6019d3a9c810_0, 0, 1;
T_3.5 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3, $push;
    .scope S_0x6019d3a823b0;
T_4 ;
    %wait E_0x6019d3a7aee0;
    %load/vec4 v0x6019d3a6a710_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/u;
    %jmp/1 T_4.0, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 32;
    %cmp/u;
    %jmp/1 T_4.1, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 32;
    %cmp/u;
    %jmp/1 T_4.2, 6;
    %dup/vec4;
    %pushi/vec4 12, 0, 32;
    %cmp/u;
    %jmp/1 T_4.3, 6;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6019d3a6a7b0_0, 0, 32;
    %jmp T_4.5;
T_4.0 ;
    %pushi/vec4 0, 0, 30;
    %load/vec4 v0x6019d3a9c750_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x6019d3a9c5b0_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x6019d3a6a7b0_0, 0, 32;
    %jmp T_4.5;
T_4.1 ;
    %load/vec4 v0x6019d3a9c670_0;
    %store/vec4 v0x6019d3a6a7b0_0, 0, 32;
    %jmp T_4.5;
T_4.2 ;
    %load/vec4 v0x6019d3a9c4d0_0;
    %store/vec4 v0x6019d3a6a7b0_0, 0, 32;
    %jmp T_4.5;
T_4.3 ;
    %load/vec4 v0x6019d3a9c3f0_0;
    %store/vec4 v0x6019d3a6a7b0_0, 0, 32;
    %jmp T_4.5;
T_4.5 ;
    %pop/vec4 1;
    %jmp T_4;
    .thread T_4, $push;
    .scope S_0x6019d3a82220;
T_5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6019d3a9d090_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6019d3a9d250_0, 0, 1;
    %end;
    .thread T_5;
    .scope S_0x6019d3a82220;
T_6 ;
    %delay 5000, 0;
    %load/vec4 v0x6019d3a9d090_0;
    %inv;
    %store/vec4 v0x6019d3a9d090_0, 0, 1;
    %jmp T_6;
    .thread T_6;
    .scope S_0x6019d3a82220;
T_7 ;
    %vpi_call 2 34 "$dumpfile", "pwm.vcd" {0 0 0};
    %vpi_call 2 35 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x6019d3a82220 {0 0 0};
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6019d3a9d250_0, 0, 1;
    %pushi/vec4 4, 0, 32;
    %store/vec4 v0x6019d3a9cc20_0, 0, 32;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0x6019d3a9cd00_0, 0, 32;
    %fork TD_pwm_tb.write, S_0x6019d3a9ca70;
    %join;
    %pushi/vec4 8, 0, 32;
    %store/vec4 v0x6019d3a9cc20_0, 0, 32;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x6019d3a9cd00_0, 0, 32;
    %fork TD_pwm_tb.write, S_0x6019d3a9ca70;
    %join;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6019d3a9cc20_0, 0, 32;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x6019d3a9cd00_0, 0, 32;
    %fork TD_pwm_tb.write, S_0x6019d3a9ca70;
    %join;
    %delay 400000, 0;
    %vpi_call 2 44 "$finish" {0 0 0};
    %end;
    .thread T_7;
